<h1>Timeline of others</h1>
<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td colspan="2"></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to('Login', new_user_session_path)  %>  

