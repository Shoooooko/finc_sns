<h1>Users一覧</h1>
<table class="table">
  <tbody>
    <% if @users.any?%>
        <% @users.each do |user| %>
            <li>
            <td><%= user.name %></td>
            <td><%= link_to "詳細ページへ", admins_user_path(user.id) %></td>
            </li>
        <% end %>
    <% end %>
    </tbody>
</table>
<%= link_to "Sign out", destroy_admin_session_path, :method => :delete %>

